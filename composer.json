{
    "name": "simpod/clickhouse-client",
    "type": "library",
    "description": "PHP ClickHouse Client",
    "keywords": [
        "clickhouse",
        "client",
        "php",
        "http",
        "HTTP client"
    ],
    "homepage": "https://github.com/simPod/ClickHouseClient",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Podlipsky",
            "email": "simon@podlipsky.net"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php-64bit": "^7.4 || ^8.0",
        "guzzlehttp/promises": "^1.4",
        "php-http/client-common": "^2.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/log": "^1.1",
        "thecodingmachine/safe": "^1.0"
    },
    "require-dev": {
        "cdn77/coding-standard": "^5.0",
        "infection/infection": "^0.20.1",
        "nyholm/psr7": "^1.4",
        "php-http/curl-client": "^2.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "0.12.81",
        "phpstan/phpstan-phpunit": "0.12.18",
        "phpstan/phpstan-strict-rules": "0.12.9",
        "phpunit/phpunit": "^9.4",
        "psalm/plugin-phpunit": "0.15.1",
        "roave/infection-static-analysis-plugin": "^1.6",
        "simpod/php-coveralls-mirror": "^3.0",
        "thecodingmachine/phpstan-safe-rule": "^1.0",
        "vimeo/psalm": "4.7.0"
    },
    "autoload": {
        "psr-4": {
            "SimPod\\ClickHouseClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimPod\\ClickHouseClient\\Tests\\": "tests/"
        }
    }
}
